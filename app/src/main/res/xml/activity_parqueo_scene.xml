<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        android:id="@+id/anima1"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000"
        motion:motionInterpolator="easeIn">
        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="90"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/auto"
                motion:percentX="0.54"
                motion:percentY="1" />
            <KeyPosition
                motion:framePosition="40"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/auto"
                motion:percentX=".5"
                motion:percentY="0.06" />
        </KeyFrameSet>
        <!--OnClick motion:targetId="@+id/auto" /-->
    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/auto"
            android:layout_width="88dp"
            android:layout_height="77dp"
            android:alpha="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="654dp" />
        <Constraint
            android:id="@+id/line_parqueo2"
            android:layout_width="100dp"
            android:layout_height="148dp"
            motion:layout_constraintEnd_toEndOf="@+id/line_parqueo"
            motion:layout_constraintTop_toBottomOf="@+id/line_parqueo" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/auto"
            android:layout_width="97dp"
            android:layout_height="75dp"
            motion:layout_constraintEnd_toEndOf="@+id/line_parqueo"
            motion:layout_constraintTop_toTopOf="@+id/line_parqueo"
            motion:layout_editor_absoluteX="314dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/start2">
        <Constraint
            android:id="@+id/auto"
            android:layout_width="88dp"
            android:layout_height="77dp"
            android:alpha="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="654dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end2">
        <Constraint
            android:id="@+id/auto"
            android:layout_width="97dp"
            android:layout_height="75dp"
            motion:layout_constraintEnd_toEndOf="@+id/line_parqueo"
            motion:layout_constraintTop_toTopOf="@+id/line_parqueo"
            motion:layout_editor_absoluteX="314dp" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/transicion2">
        <Constraint
            android:id="@+id/auto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>
    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/transicion2"
        motion:constraintSetStart="@+id/end"
        motion:duration="3000">

        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="5"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/auto"
                motion:percentX="0.5" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/auto"
                motion:percentX="0.4" />

            <KeyPosition
                motion:framePosition="90"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/auto"
                motion:percentX="0.5" />


        </KeyFrameSet>

    </Transition>
</MotionScene>